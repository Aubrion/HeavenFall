///lynx_element_get_color(argument0)
var A;
switch (argument0)
{
    case 0: A = c_orange; break;
    case 1: A = c_navy; break;
    case 2: A = c_yellow; break;
    case 3: A = c_white; break;
    case 4: A = c_blue; break;
    case 5: A = c_green; break;
    case 6: A = c_red; break;
    default: A = c_white; break;
}

return A;
